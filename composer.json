{
  "name": "yuncms/yii2-credit",
  "description": "credit",
  "keywords": [
    "credit"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Xu TL",
      "email": "xutongle@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yiisoft/yii2-queue": "~2.0.0",
    "yuncms/yii2-user": "~2.0"
  },
  "autoload": {
    "files": [
      "helpers.php"
    ],
    "psr-4": {
      "yuncms\\credit\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "credit",
      "i18n": {
        "class": "yii\\i18n\\PhpMessageSource",
        "basePath": "@yuncms/credit/message"
      },
      "migrationNamespace": "yuncms\\credit\\migrations",
      "backend": {
        "class": "yuncms\\credit\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\credit\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
